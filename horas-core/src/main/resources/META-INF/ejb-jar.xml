<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar
	version="3.1"
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
						http://java.sun.com/xml/ns/javaee/ejb-jar_3_1.xsd">
	<enterprise-beans>
	    <session>
	        <ejb-name>UsuarioBusinessImpl</ejb-name>
			<env-entry>
				<env-entry-name>email.assunto</env-entry-name>
				<env-entry-type>java.lang.String</env-entry-type>
				<env-entry-value>Horas Apontamento - Usuário Criado</env-entry-value>
			</env-entry>
			<env-entry>
				<env-entry-name>email.corpo.template</env-entry-name>
				<env-entry-type>java.lang.String</env-entry-type>
				<env-entry-value>
					Olá {0}, 
					
					seu usuário foi criado. Sua senha de acesso é: {1}
				</env-entry-value>
			</env-entry>
	    </session>
	</enterprise-beans>
</ejb-jar>